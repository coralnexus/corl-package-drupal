{
  "builders": {
    "package": {
      "environment": {
        "production": {
          "coralnexus__web": "github:::coralnexus/corl-package-web[46f4cb4ae883aaffd3df3ef3a64b5a62ce7c51c6]"
        },
        "qa": {
          "use": "production"
        },
        "default": {
          "coralnexus__web": "github:::coralnexus/corl-package-web"
        }
      }
    }
  },
  "provisioners": {
    "puppetnode": {
      "coralnexus::web": {
        "profiles": {
          "varnish_server": {
            "environment": {
              "production": {
                "modules": {
                  "varnish_drupal": "github:::coralnexus/puppet-varnish_drupal[befe9c968e3bc5cd5fb322b30a34f587fd75aa5d]"
                }
              },
              "default": {
                "modules": {
                  "varnish_drupal": "github:::coralnexus/puppet-varnish_drupal"
                }
              }
            }
          }
        }
      },
      "coralnexus::drupal": {
        "profiles": {
          "apache_drupal": {
            "extend": [ "coralnexus::web::profile::apache_server", "coralnexus::core::profile::php" ],
            "environment": {
              "production": {
                "modules": {
                  "drupal": "github:::coralnexus/puppet-drupal[62d5b9ff045308257be7bca7df5f2462bf7f1300]"
                }
              },
              "qa": {
                "use": "production"
              },
              "default": {
                "modules": {
                  "drupal": "github:::coralnexus/puppet-drupal"
                }
              }
            }
          }
        }
      }
    }
  }
}
